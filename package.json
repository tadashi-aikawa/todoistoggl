{
  "name": "todoistoggl",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "refresh": "npm prune && npm i && typings prune && typings i",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "build": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:pro": "webpack --config webpack.config.production.js",
    "start:dev": "cross-env NODE_ENV=development electron electron",
    "start": "cross-env NODE_ENV=development electron dist",
    "start:pro": "electron dist",
    "package": "npm run build:pro && electron-packager dist todoistoggl --platform=win32 --arch=x64 --overwrite",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tadashi-aikawa/todoistoggl.git"
  },
  "author": "tadashi-aikawa",
  "bugs": {
    "url": "https://github.com/tadashi-aikawa/todoistoggl/issues"
  },
  "homepage": "https://github.com/tadashi-aikawa/todoistoggl#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "electron-packager": "^7.5.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.3",
    "query-string": "^4.2.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "electron-devtools-installer": "^2.0.0",
    "electron-prebuilt": "^1.3.2",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.1",
    "react-hot-loader": "^1.3.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.14.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
